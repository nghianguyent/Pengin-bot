{
	"singleQuote": true,
	"trailingComma": "all",
	"printWidth": 80,
	"tabWidth": 4,
	"semi": true,
	"bracketSpacing": true,
	"endOfLine": "auto",
	"arrowParens": "avoid",
	"useTabs": true,
	"importOrder": [
		"^@core/(.*)$",
		"^@server/(.*)$",
		"^@client/(.*)$",
		"^@utils/(.*)$",
		"^@types/(.*)$",
		"^@/(.*)$",
		"^[./]"
	],
	"plugins": ["prettier-plugin-organize-imports"]
}
